<%@ Page Title="StartUp Policy Detail Directorate of ICT & e-Governance" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeFile="StartUp_Policy_Detail.aspx.cs" Inherits="PolicyDetails_StartUp_Policy_Detail" %>
<asp:Content runat="server" ID="BodyContent" ContentPlaceHolderID="MainContent">
 
